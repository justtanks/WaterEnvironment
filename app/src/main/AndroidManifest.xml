<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.waterenvironment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/QMUI.Compat.NoActionBar">
        <activity
            android:name=".activity.WelActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_wel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".activity.ForgetActivity"
            android:label="@string/title_activity_forget" />
        <activity
            android:name=".activity.NewPaawordActivity"
            android:label="@string/title_activity_new_paaword" />
        <activity
            android:name=".person.ChangeUserImgAcivity"
            android:label="@string/title_activity_change_user_img_acivity" />
        <activity
            android:name=".person.ChangePasswordActivity"
            android:label="@string/title_activity_change_password" />
        <activity
            android:name=".person.SetActivity"
            android:label="@string/title_activity_set"></activity>
    </application>

</manifest>